{
  "name": "nopreme",
  "version": "0.0.0",
  "description": "NO MORE PREMIUM",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "NODE_ENV=development babel-node server.js",
    "relay": "NODE_ENV=development relay-compiler --src ./src --schema ./data/schema.graphql --artifactDirectory ./__generated__/relay",
    "clean-relay": "rm -rf __generated__",
    "build": "NODE_ENV=development parcel build src/entries/**/*.html src/entries/*.html -d docs/",
    "clean": "rm -rf dist docs .cache",
    "update-schema": "NODE_ENV=development babel-node ./scripts/updateSchema.js",
    "help": "babel-node --help",
    "watch": "NODE_ENV=development parcel watch src/entries/**/*.html src/entries/*.html -d docs/"
  },
  "keywords": [],
  "author": "Sanghoon Yoon",
  "license": "ISC",
