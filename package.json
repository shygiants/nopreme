    "@afuggini/namegenerator": "^0.1.3",
    "@babel/runtime": "^7.8.3",
    "@google-cloud/storage": "^4.3.0",
    "babel-plugin-relay": "^8.0.0",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "express-jwt": "^5.3.1",
    "farce": "^0.2.8",
    "found": "^0.4.9",
    "found-relay": "^0.6.0",
    "graphql": "^14.5.8",
    "graphql-relay": "^0.6.0",
    "grommet": "^2.10.0",
    "grommet-icons": "^4.4.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "lodash.debounce": "^4.0.8",
    "mongoose": "^5.8.9",
    "multer": "^1.4.2",
    "polished": "^3.4.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-relay": "^8.0.0",
    "react-relay-network-modern": "^4.4.0",
    "relay-compiler": "^8.0.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.8",
    "styled-components": "^5.0.1",
    "uuid": "^3.4.0"
  },
